#include <iostream>
#include <cstdio>
#include <cstring>
#include <cstdlib>
#include <cmath>
#include <algorithm>
#include <sstream>
#include <fstream>
#include <string>
#include <queue>
#include <stack>
#include <vector>
#include <map>
#include <set>

#define LL long long
#define debug cout<<"Error!"
#define INF 0x3f3f3f3f
#define PI atan(1.0)*4

using namespace std;
int max(int a, int b);
int main()
{
	int n, a[100005], b[100005];
	cin >> n;
	for (int i = 0; i < n; i++)
	{
		cin >> a[i];
	}
	for (int i = 0; i < n; i++)
	{
		cin >> b[i];
	}
	int i = 0, j = 0, count = 0;
	int flag = 0;
	while (i<n&&j<n)
	{
		if (a[i] == b[j]) {
			count++;
			i++;
			flag = 0;
		}
		else if(a[i]<b[j]){
			i++;
			count++;
			flag = 1;
		}
		else {
			j++;
			count++;
		}
		if (count == (2*n - 1) / 2)break;
	}
	if (n==1) {
		cout << (a[0] < b[0] ? a[0] : b[0]);
	}
	else if (flag) {
		cout << a[i];
	}
	else {
		cout << b[j];
	}


	return 0;
}
